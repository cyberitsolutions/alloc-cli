#!/usr/bin/env python

import sys
from alloccli import alloc

# Try to import relevant module
try:
  m = sys.argv[1]
  acli = __import__("alloccli."+m)
  subcommand = getattr(getattr(acli,m), m)

# Else bomb out with help message
except:
  alloc.alloc().get_cli_help()

# All clear, run the module
s = subcommand()
s.run(sys.argv)
